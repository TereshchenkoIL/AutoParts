<Window x:Class="AutoParts.Select_Part"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoParts"
        mc:Ignorable="d"
        Title="Select_Part" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="Grid" HorizontalAlignment="Left" Height="310" Margin="30,70,0,0" VerticalAlignment="Top" Width="730">
           
        </DataGrid>
        <Button Content="Button" HorizontalAlignment="Left" Margin="280,25,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <ComboBox x:Name="Car_Box" HorizontalAlignment="Left" Margin="30,20,0,0" VerticalAlignment="Top" Width="160" SelectionChanged="Car_Box_SelectionChanged"/>

    </Grid>
</Window>
